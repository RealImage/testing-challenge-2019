<testCases>    
	<testCase>
        <description>upload a file with invalid token</description>
        <url>/sharebox/api/upload</url>
        <requestMethod>POST</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <multipartFormData>src/test/resources/testfiles/testfile1.txt</multipartFormData>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase>
    
    <testCase>
        <description>Get uploaded file details invalid token</description>
        <url>/sharebox/api/upload</url>
        <requestMethod>GET</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase>
    
    <testCase>
        <description>update an uploaded file with invalid token</description>
        <url>/sharebox/api/upload</url>
        <requestMethod>PUT</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <formData>
            <entry>
                <key>fileId</key>
                <value>a44740c1-6e64-437c-8349-72f11fd24ec0</value>
            </entry>
            <entry>
                <key>bytesCompleted</key>
                <value>40</value>
            </entry>
        </formData>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase> 

	<testCase>
        <description>share a file with invalid token</description>
        <url>/sharebox/api/files</url>
        <requestMethod>POST</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <formData>
            <entry>
                <key>fileId</key>
                <value>a44740c1-6e64-437c-8349-72f11fd24ec0</value>
            </entry>
            <entry>
                <key>shareTo</key>
                <value>#user2Id</value>
            </entry>
        </formData>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase> 
    
    <testCase>
        <description>get list of files with invalid token</description>
        <url>/sharebox/api/files</url>
        <requestMethod>GET</requestMethod>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase> 
    
    <testCase>
        <description>accept or reject file with invalid token</description>
        <url>/sharebox/api/files</url>
        <requestMethod>PUT</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <formData>
            <entry>
                <key>fileId</key>
                <value>a44740c1-6e64-437c-8349-72f11fd24ec0</value>
            </entry>
            <entry>
                <key>isAccepted</key>
                <value>false</value>
            </entry>
        </formData>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase> 
    
    <testCase>
        <description>delete a file with invalid token</description>
        <url>/sharebox/api/files</url>
        <requestMethod>DELETE</requestMethod>
        <headers>
	        <entry>
	            <key>Accept</key>
	            <value>application/json</value>
	        </entry>
    	</headers>
        <parameters>
            <entry>
                <key>token</key>
                <value>b817aadc-e554-41ce-ab9f-e58bb60847ea</value>
            </entry>
        </parameters>
        <formData>
            <entry>
                <key>fileId</key>
                <value>a44740c1-6e64-437c-8349-72f11fd24ec0</value>
            </entry>
        </formData>
        <expectedStatusCode>401</expectedStatusCode>
    </testCase> 
</testCases>